{
  "name": "js-core-data",
  "description": "ORM inspired by Apple's Objective-C CoreData",
  "version": "1.6.1",
  "license": "MIT",
  "keywords": [
    "ORM",
    "CoreData",
    "object relational mapper",
    "context",
    "sqlite",
    "mysql"
  ],
  "dependencies": {
    "array-control": "^0.2.0",
    "async": "^2.0.1",
    "bluebird": "^3.3.4",
    "generic-pool": "^2.2.0",
    "js-yaml": "^3.3.1",
    "lock": "^0.1.2",
    "moment": "^2.11.2",
    "squel": "^5.0.2",
    "string": "^3.3.1",
    "underscore": "^1.8.3",
    "underscore.inflections": "^0.2.1",
    "unit-converter": "^1.0.2",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "js-sha512": "^0.2.2",
    "mocha": "^3.0.0",
    "mysql": "^2.8.0",
    "passport": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^6.0.3",
    "sqlite3": "^3.0.9",
    "supertest": "^2.0.0",
    "tmp": "0.0.28"
  },
  "scripts": {
    "deploy": "git push;git push --tags;npm publish",
    "test-mysql": "env STORAGE=mysql mocha test;",
    "test-sqlite": "env STORAGE=sqlite mocha test;",
    "test-postgres": "env STORAGE_URL=postgres://postgres@localhost/test mocha test",
    "test-postgres-native": "env NODE_ENV=production STORAGE_URL=postgres://postgres@localhost/test mocha test",
    "test": "npm run test-sqlite;npm run test-mysql;npm run test-postgres;npm run test-postgres-native"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jakubknejzlik/js-core-data.git"
  },
  "browserify": {
    "transform": []
  }
}
